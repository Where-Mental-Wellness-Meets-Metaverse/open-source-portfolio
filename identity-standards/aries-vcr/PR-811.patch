+  t.equal(URI.resolve(base, '../../../g'), 'uri://a/g', '../../../g')
+  t.equal(URI.resolve(base, '../../../../g'), 'uri://a/g', '../../../../g')
+
+  t.equal(URI.resolve(base, '/./g'), 'uri://a/g', '/./g')
+  t.equal(URI.resolve(base, '/../g'), 'uri://a/g', '/../g')
+  t.equal(URI.resolve(base, 'g.'), 'uri://a/b/c/g.', 'g.')
+  t.equal(URI.resolve(base, '.g'), 'uri://a/b/c/.g', '.g')
+  t.equal(URI.resolve(base, 'g..'), 'uri://a/b/c/g..', 'g..')
+  t.equal(URI.resolve(base, '..g'), 'uri://a/b/c/..g', '..g')
+
+  t.equal(URI.resolve(base, './../g'), 'uri://a/b/g', './../g')
