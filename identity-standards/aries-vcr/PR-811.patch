+  t.equal(URI.resolve(base, 'g/../h'), 'uri://a/b/c/h', 'g/../h')
+  t.equal(URI.resolve(base, 'g;x=1/./y'), 'uri://a/b/c/g;x=1/y', 'g;x=1/./y')
